    "lint:fix": "eslint lib test --fix",
    "cli": "node bin/cli.js",
    "example": "node examples/node-usage.js"
  },
  "keywords": [
    "spec-up-t",
    "health-check",
    "validation",
    "specification",
    "markdown",
    "cli",
    "npm",
    "quality-assurance"
  ],
  "author": "Blockchain Bird",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/blockchainbird/spec-up-t-healthcheck.git"
  },
  "bugs": {
    "url": "https://github.com/blockchainbird/spec-up-t-healthcheck/issues"
  },
  "homepage": "https://github.com/blockchainbird/spec-up-t-healthcheck#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "lib/",
    "bin/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "axios": "^1.5.0",
    "commander": "^11.0.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "eslint": "^8.44.0",
    "jest": "^29.6.0",
    "nodemon": "^3.0.0"
  },
  "peerDependencies": {
    "node-fetch": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "node-fetch": {
      "optional": true
    }
